{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/api/health": {
            "get": {
                "description": "Healthcheck route",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/api.HealthResponse"
                        }
                    },
                    "503": {
                        "description": "Service Unavailable",
                        "schema": {
                            "$ref": "#/definitions/api.HealthResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/cats": {
            "get": {
                "description": "Find cats route",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "cat"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/cat.ListCatsDto"
                        }
                    }
                }
            },
            "post": {
                "description": "Create cat route",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "cat"
                ],
                "parameters": [
                    {
                        "description": "Cat",
                        "name": "cat",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/cat.CatCmd"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/cat.CatDto"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/api.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "api.ErrorResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "Invalid request"
                }
            }
        },
        "api.HealthResponse": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string"
                }
            }
        },
        "cat.CatCmd": {
            "type": "object",
            "properties": {
                "birthday": {
                    "type": "string",
                    "example": "2000-01-01"
                },
                "name": {
                    "type": "string",
                    "example": "Mimoso"
                }
            }
        },
        "cat.CatDto": {
            "type": "object",
            "properties": {
                "birthday": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "cat.ListCatsDto": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/cat.CatDto"
                    }
                }
            }
        }
    }
}